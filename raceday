let raceNumber = Math.floor(Math.random() * 1000);

const registerEarly = false;
const runnerAge = 18;

if (registerEarly && runnerAge > 18) {
  raceNumber += 1000;
}

if (registerEarly && runnerAge > 18) {
  console.log(`Runner ${raceNumber} you will start at 9:30 am.`);
} else if (!registerEarly && runnerAge > 18) {
  console.log(`Runner ${raceNumber} you will start at 11:00 am.`)
} else if (runnerAge < 18) {
  console.log(`Runner ${raceNumber} you will start at 12:30 am.`)
} else {
  console.log(`Runner ${raceNumber} please come to the registration desk`)
}
